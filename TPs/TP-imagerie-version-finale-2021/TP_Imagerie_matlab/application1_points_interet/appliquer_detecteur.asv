function appliquer_detecteur
global popupMethodedetect editSeuille A
det=get(popupMethodedetect,'value');

 if det==1
        str=get(editSeuille,'String');
        thresh=Str2num(char(str));
        harris(A,thresh);
        ajouter_bruit(A);
 end
 if det==2
        str=get(editSeuille,'String');
        thresh=Str2num(char(str));
        susan(A,thresh);
        ajouter_bruit(A);
 end